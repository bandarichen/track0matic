# vim: ft=python

Import('env')

envCopy = env.Clone()

envCopy.Append(
                CPPPATH = '../src/',
              )

controllerDir = 'Controller'

sourceTargets = [ 'maincontroller.cpp' ]

targets = []
for source in sourceTargets:
  targets.append(controllerDir + '/' + source)

library = envCopy.Library('Controller',targets)

Return( 'library' )
