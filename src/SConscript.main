# vim: ft=python

Import('qtEnv')

envCopy = qtEnv.Clone()

envCopy.Append(
                CPPPATH = '../src/',
              )

targets = [ 'main.cpp' ]

program = envCopy.Program(target = '../bin/Tracker',
                          source = targets,
                          LIBS = [ 'QtView', 'Controller', 'Common', 'Model', 'QtCore', 'QtGui' ],
                          LIBPATH = ['.','/usr/lib64/qt4'])

Return( 'program' )
