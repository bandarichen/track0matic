# vim: ft=python

Import('env')

viewDir = 'View'

envCopy = env.Clone()
envCopy.Append(
                CPPPATH = '.'
              )

sourceTargets = [ 'gnuplot.cpp' ]

targets = []
for source in sourceTargets:
  targets.append(viewDir + '/' + source)

library = envCopy.Library('GNUPlot',targets)

Return( 'library' )
